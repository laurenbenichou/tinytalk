<div id="<%= board.id %>" class="box" >
  <div id="title_<%= board.id %>">
    <div class="box-img">
    <% for(var index in users){ %>
      <% if (users[index].avatar_file_name == null){ %>
       <img id ="avatar-update" src="/assets/missing.jpg">
     <% } else {%>
         <img id ="avatar-update" src="http://s3-us-west-1.amazonaws.com/tinytalk/users/avatars/000/000/0<%=users[index].id%>/original/<%= users[index].avatar_file_name %>">
      <%}%>
    <%}%>
    </div>
  <hr>
  <h1 class="box-headings"><a href="/boards/<%= board.id %>"><%= board.title %></a></h1>
  </div>
  <hr>
  <div class="box-button">
  <button class="btn btn-default btn-sm" data-id="<%= board.id  %>" data-method="delete">Delete</button>

  <button class="btn btn-default btn-sm" data-id="<%= board.id  %>" data-toggle="modal" data-target="#myModal2"> update board</button>
  </div>
</div>

